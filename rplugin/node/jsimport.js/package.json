{
  "name": "jsimport-rplugin.nvim",
  "version": "0.0.1",
  "description": "Magical autocompletion for es6 imports in neovim",
  "main": "lib/index.js",
  "scripts": {
    "dev": "babel src --out-dir lib --ignore '__tests__' --watch --source-maps inline",
    "build": "babel src --out-dir lib --ignore '__tests__' --source-maps inline",
    "test": "jest"
  },
  "keywords": [
    "es6",
    "neovim",
    "vim",
    "autocomplete"
  ],
  "author": "Billy Vong <billy at mmo.me>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "jest": "^18.1.0"
  },
  "dependencies": {
    "babel-traverse": "^6.21.0",
    "babylon": "^6.14.1",
    "lodash": "^4.17.2",
    "mz": "^2.6.0",
    "raven": "^1.1.1",
    "regenerator-runtime": "^0.10.1",
    "update-notifier": "^1.0.3"
  },
  "jest": {
    "rootDir": "src",
    "testPathIgnorePatterns": [
      "eslintrc"
    ]
  }
